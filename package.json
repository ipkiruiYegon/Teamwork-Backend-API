{
  "name": "teamwork-backend-api",
  "version": "1.0.0",
  "description": "API server for a internal social network",
  "main": "./src/index.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "server": "babel-node ./src/index.js",
    "serve": "NODE_ENV=production exec babel-node ./dist/index.js",
    "start": "npm run prod",
    "server:prod": "node ./dist/index.js",
    "dev": "NODE_ENV=development DEBUG=teamwork-backend-api:debug npm-run-all server",
    "dbTables": "node db.js createTables",
    "dropTables": "node db.js dropTables",
    "dbAlterTables": "node db.js alterTables",
    "dbUser": "node db.js createSuperUser",
    "dbNormalUser": "node db.js createNormalUser",
    "migrateDev": "NODE_ENV=development npm-run-all dbTables dbAlterTables dbUser",
    "migrateProd": "NODE_ENV=production npm-run-all dbTables dbAlterTables dbUser",
    "clean": "rimraf dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "testDb": "npm-run-all dropTables dbTables dbAlterTables dbUser dbNormalUser",
    "mochaTest": "mocha tests/ --reporter  spec --exec babel-node --require @babel/register --exit",
    "test": "NODE_ENV=test DEBUG=teamwork-backend-api:debug npm-run-all testDb mochaTest",
    "travis": "NODE_ENV=travis DEBUG=teamwork-backend-api:debug  npm-run-all testDb mochaTest",
    "test:watch": "mocha tests/ --reporter  spec --NODE_ENV=development --DEBUG=server:debug --exec --require @babel/register babel-node  watch",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\"",
    "lint": "eslint",
    "watch": "NODE_ENV=development DEBUG=teamwork-backend-api:debug nodemon ./src/index.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipkiruiYegon/Teamwork-Backend-API.git"
  },
  "author": "Yegon Kipkirui Geoffrey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ipkiruiYegon/Teamwork-Backend-API/issues"
  },
  "homepage": "https://github.com/ipkiruiYegon/Teamwork-Backend-API#readme",
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "config": "^3.2.4",
    "core-util-is": "^1.0.2",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "nodemailer": "^6.4.2",
    "npm-run-all": "^4.1.5",
    "pg": "^7.14.0",
    "randomstring": "^1.1.5",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.7.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "mocha": "^6.2.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.1",
    "supertest": "^4.0.2"
  }
}
